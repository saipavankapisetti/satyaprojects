setTimeout(function(){const offsetWidth=document.documentElement.offsetWidth>770?document.documentElement.offsetWidth-300:document.documentElement.offsetWidth,slideshowContainer=document.querySelectorAll(".slideshow")[0],slideLinkContainers=document.querySelectorAll(".slide__link"),halfContainers=document.querySelectorAll(".banner__media-half");slideshowContainer.style.height=offsetWidth/4+"px";for(let i=0;i<slideLinkContainers.length;i++)slideLinkContainers[i].style.height=offsetWidth/4+"px";for(let i=0;i<halfContainers.length;i++)halfContainers[i].style.height=offsetWidth/8+"px",halfContainers[i].closest("div.banner").style.height=offsetWidth/8+"px"},500);function changeButtonText(){try{const buttons=document.querySelectorAll(".shopify-payment-button__button.shopify-payment-button__button--unbranded:not(.shopify-payment-button__button--hidden)");for(let i=0;i<buttons.length;i++)buttons[i].textContent="Buy Now"}catch(err){console.log(err)}}setTimeout(changeButtonText,2e3);
